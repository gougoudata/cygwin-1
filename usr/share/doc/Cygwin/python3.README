  crypt-1.1-1
  libbz2_1-1.0.5-10
  libexpat1-2.0.1-1
  libffi4-4.3.4-3
  libgcc1-4.3.4-3
  libgdbm4-1.8.3-20
  libintl8-0.17-11
  libncursesw10-5.7-18
  libreadline7-6.0.3-2
  libsqlite3_0-3.6.21-3
  openssl-0.9.8n-1
