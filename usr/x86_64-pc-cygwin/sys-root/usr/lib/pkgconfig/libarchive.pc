prefix=/usr/x86_64-pc-cygwin/sys-root/usr
exec_prefix=/usr/x86_64-pc-cygwin/sys-root/usr
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: libarchive
Description: library that can create and read several streaming archive formats
Version: 3.1.2
Cflags: -I${includedir}
Libs: -L${libdir} -larchive
Libs.private: -lnettle -lbz2 -lz  -L/usr/x86_64-pc-cygwin/sys-root/usr/lib -lxml2 -lz -liconv -lm
