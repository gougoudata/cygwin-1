prefix=/usr
exec_prefix=/usr
libdir=${exec_prefix}/lib
APR_MAJOR_VERSION=1
includedir=${exec_prefix}/include/apr-${APR_MAJOR_VERSION}

Name: APR
Description: The Apache Portable Runtime library
Version: 1.4.6
Libs: -L${libdir} -lapr-${APR_MAJOR_VERSION} -luuid -lcrypt 
Cflags: -DCYGWIN  -I${includedir}
