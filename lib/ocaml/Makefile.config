# generated by ./configure -bindir /usr/bin -libdir /usr/lib/ocaml -mandir /usr/share/man -no-tk
PREFIX=/usr/local
BINDIR=/usr/bin
LIBDIR=/usr/lib/ocaml
STUBLIBDIR=$(LIBDIR)/stublibs
MANDIR=/usr/share/man
MANEXT=1
RANLIB=ranlib
RANLIBCMD=ranlib
SHARPBANGSCRIPTS=false
BNG_ARCH=ia32
BNG_ASM_LEVEL=2
PTHREAD_LINK=-cclib -lpthread
X11_INCLUDES=
X11_LINK=-lX11
DBM_INCLUDES=
DBM_LINK=-lgdbm_compat -lgdbm
TK_DEFS=
TK_LINK=
LIBBFD_LINK=
BYTECC=gcc
BYTECCCOMPOPTS=-fno-defer-pop -Wall -U_WIN32 -D_FILE_OFFSET_BITS=64 -D_REENTRANT
BYTECCLINKOPTS=
BYTECCLIBS= -lm  -lcurses -lpthread
BYTECCRPATH=
EXE=.exe
SUPPORTS_SHARED_LIBRARIES=true
SHAREDCCCOMPOPTS=
MKSHAREDLIBRPATH=
NATDYNLINKOPTS=
SYSLIB=-l$(1)
#ml let syslib x = "-l"^x;;

### How to build a static library
MKLIB=ar rc $(1) $(2); ranlib $(1)
#ml let mklib out files opts = Printf.sprintf "ar rc %s %s %s; ranlib %s" out opts files out;;
ARCH=i386
MODEL=default
SYSTEM=cygwin
NATIVECC=gcc
NATIVECCCOMPOPTS=-Wall -U_WIN32 -D_FILE_OFFSET_BITS=64 -D_REENTRANT
NATIVECCPROFOPTS=-Wall -U_WIN32 -D_FILE_OFFSET_BITS=64 -D_REENTRANT
NATIVECCLINKOPTS=
NATIVECCRPATH=
NATIVECCLIBS= -lm 
ASM=as
ASPP=gcc -c
ASPPPROFFLAGS=-DPROFILING
PROFILING=noprof
DYNLINKOPTS=
OTHERLIBRARIES=unix str num dynlink bigarray systhreads threads graph dbm
DEBUGGER=ocamldebugger
CC_PROFILE=-pg
SYSTHREAD_SUPPORT=true
PARTIALLD=ld -r
PACKLD=$(PARTIALLD) $(NATIVECCLINKOPTS) -o\ 
DLLCCCOMPOPTS=-U_WIN32 -DCAML_DLL
IFLEXDIR=-I"/usr/lib/flexdll"
O=o
A=a
SO=so
EXT_OBJ=.o
EXT_ASM=.s
EXT_LIB=.a
EXT_DLL=.so
EXTRALIBS=
CCOMPTYPE=cc
TOOLCHAIN=cc
NATDYNLINK=true
CMXS=cmxs
MKEXE=flexlink -chain cygwin -merge-manifest -exe
MKDLL=flexlink -chain cygwin -merge-manifest
MKMAINDLL=flexlink -chain cygwin -merge-manifest -maindll
