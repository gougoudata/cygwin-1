#!/bin/bash

prefix=/usr
bindir=${prefix}/bin
sbindir=${prefix}/sbin
infodir=${prefix}/share/info

/usr/sbin/update-alternatives \
    --install "${bindir}/g77.exe" "g77" "${bindir}/g77-3.exe" 30 \
  --slave "/usr/bin/f77.exe" "f77" "/usr/bin/f77-3.exe" \
  --slave "/usr/share/man/man1/g77.1.gz" "g77.1.gz" "/usr/share/man/man1/g77-3.1.gz" \


